#keybindings for programs
super + Return
	$TERMINAL
super + shift + t 
	$TERMINAL
super + b
	{$BROWSER}
super + o
    xournalpp

#generic
super + {_,shift} + a
	{$TERMINAL -e pulsemixer,sound.sh mute}
super + {_,shift} + p
	{sound.sh up 10,sound.sh dowm 10}

#menu keybindings
super + {_,shift} + x
	{fin.sh,xkill}
super + {_,shift} + r
    {dmenu_run -fn 'Hack-15',screen.sh}
super + g
	passmenu -i
